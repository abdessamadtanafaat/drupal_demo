{
    "name": "drupal/recommended-project",
    "description": "Project template for Drupal projects with a relocated document root",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://git.drupalcode.org/issue/responsive_favicons-3434213.git",
            "priority": 10
        },
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8",
            "priority": 1

        }
    ],
    "require": {
        "composer/installers": "^2.3",
        "drupal/asset_injector": "^2.21",
        "drupal/auto_entityqueue": "^3.0",
        "drupal/better_exposed_filters": "^7.0",
        "drupal/block_field": "^1.0@RC",
        "drupal/coffee": "^2.0",
        "drupal/core-composer-scaffold": "^11.1",
        "drupal/core-project-message": "^11.1",
        "drupal/core-recommended": "^11.1",
        "drupal/default_content": "^2.0@alpha",
        "drupal/devel": "^5.3",
        "drupal/eck": "^2.1@beta",
        "drupal/email_registration": "^2.0@RC",
        "drupal/entityqueue": "^1.9",
        "drupal/eu_cookie_compliance": "^1.25",
        "drupal/field_group": "^3.6",
        "drupal/file_entity": "^2.1",
        "drupal/flood_control": "^3.0",
        "drupal/google_tag": "^2.0",
        "drupal/inline_entity_form": "^3.0@RC",
        "drupal/lang_dropdown": "^2.3",
        "drupal/linkit": "^7.0",
        "drupal/login_security": "^2.0",
        "drupal/metatag": "^2.1",
        "drupal/minifyhtml": "^2.0",
        "drupal/paragraphs": "^1.18",
        "drupal/path_redirect_import": "^2.1",
        "drupal/pathauto": "^1.13",
        "drupal/persistent_login": "^2.2",
        "drupal/recaptcha": "^3.4",
        "drupal/redirect": "^1.11",
        "drupal/responsive_favicons": "dev-project-update-bot-only as 2.x-dev",
        "drupal/scheduler": "^2.2",
        "drupal/search_api": "^1.38",
        "drupal/seckit": "^2.0",
        "drupal/security_review": "^3.1",
        "drupal/shield": "^1.8",
        "drupal/simple_sitemap": "^4.2",
        "drupal/smart_trim": "^2.2",
        "drupal/social_auth_google": "^4.0",
        "drupal/symfony_mailer": "^2.0@alpha",
        "drupal/telephone_validation": "^2.5",
        "drupal/ui_patterns": "^2.0",
        "drupal/ultimate_cron": "^2.0@beta",
        "drupal/webform": "^6.3@beta",
        "drush/drush": "^13.3"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true,
            "phpstan/extension-installer": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "php-http/discovery": true
        },
        "sort-packages": true
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ],
            "recipes/{$name}": [
                "type:drupal-recipe"
            ]
        },
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the Drupal codebase  </>",
                "<bg=blue;fg=white>  from the drupal/recommended-project template!          </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",
                "  * Install the site: https://www.drupal.org/docs/installing-drupal",
                "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
                "  * Get support: https://www.drupal.org/support",
                "  * Get involved with the Drupal community:",
                "      https://www.drupal.org/getting-involved",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        }
    }
}
